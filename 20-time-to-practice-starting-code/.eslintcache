ar/React-TheCompleteGuide/20-time-to-practice-starting-code/src/pages/QuoteDetail.js",["90"],"import { Fragment, useEffect } from \"react\";\nimport { Route, useParams, useRouteMatch } from \"react-router\";\nimport useHttp from \"../hooks/use-http\";\nimport { getSingleQuote } from \"../lib/api\";\n\nimport HighlightedQuote from \"../components/quotes/HighlightedQuote\";\n\nimport Comments from \"../components/comments/Comments\";\nimport { Link } from \"react-router-dom\";\nimport LoadingSpinner from \"../components/UI/LoadingSpinner\";\n\nconst DUMMY_QUOTES = [\n  { id: \"q1\", author: \"Max\", text: \"Learning React is fun!\" },\n  { id: \"q1\", author: \"Maxmilian\", text: \"Learning React is great!\" },\n];\n\nconst QuoteDetail = () => {\n  const params = useParams();\n  const match = useRouteMatch();\n\n  const { quoteId } = params;\n  console.log(quoteId);\n\n  const {\n    sendRequest,\n    status,\n    data: loadedQuote,\n    error,\n  } = useHttp(getSingleQuote, true);\n\n  useEffect(() => {\n    sendRequest(quoteId);\n  }, [sendRequest, quoteId]);\n\n  if (status === \"pending\") {\n    return (\n      <div className=\"centered\">\n        <LoadingSpinner />\n      </div>\n    );\n  }\n\n  if (error) {\n    return <p className=\"centered\">{error}</p>;\n  }\n\n  if (!loadedQuote.text) {\n    return <p>No quote found!</p>;\n  }\n\n  return (\n    <Fragment>\n      <HighlightedQuote text={loadedQuote.text} author={loadedQuote.author} />\n      <Route path={match.path} exact>\n        <div className=\"centered\">\n          <Link className=\"btn--flat\" to={`${match.url}/comments`}>\n            Load Comments\n          </Link>\n        </div>\n      </Route>\n      <Route path={`${match.path}/comments`}>\n        <Comments />\n      </Route>\n    </Fragment>\n  );\n};\n\nexport default QuoteDetail;\n","/home/tomble/Documents/Catatan_Belajar/React-TheCompleteGuide/20-time-to-practice-starting-code/src/pages/NewQuote.js",[],"/home/tomble/Documents/Catatan_Be